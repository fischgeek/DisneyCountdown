{
	"version": "0.2.0",
	"configurations": [{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "${workspaceRoot}\\Index.html",
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "Launch currently open script",
			"type": "php",
			"request": "launch",
			"program": "${file}",
			"cwd": "${fileDirname}",
			"port": 9000
		}
	]
}